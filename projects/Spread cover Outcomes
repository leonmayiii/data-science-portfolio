    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "df = pd.read_csv(r\"C:\\Users\\pheno\\OneDrive\\Desktop\\DSC550\\nba_2008-2025.csv\")\n",
    "\n",
    "\n",
    "df[\"date\"] = pd.to_datetime(df[\"date\"])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "8189b9b0-f74e-4fe6-9102-e694e54f492f",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Final margin of victory (home perspective)\n",
    "df[\"margin\"] = df[\"score_home\"] - df[\"score_away\"]\n",
    "\n",
    "# Normalize the spread so positive = home favorite\n",
    "df[\"spread_home\"] = np.where(df[\"whos_favored\"] == \"home\",\n",
    "                             df[\"spread\"],         # home favorite\n",
    "                             -df[\"spread\"])        # home underdog\n",
    "\n",
    "# ATS residual and cover flag\n",
    "df[\"ats_residual\"] = df[\"margin\"] - df[\"spread_home\"]\n",
    "df[\"cover\"] = (df[\"ats_residual\"] > 0).astype(int)\n",
    "\n",
    "# Implied probability from American odds\n",
    "def implied_prob(odds):\n",
    "    return np.where(odds < 0,\n",
    "                    -odds / (-odds + 100),\n",
    "                     100 / (odds + 100))\n",
    "df[\"imp_home_win\"] = implied_prob(df[\"moneyline_home\"])\n",
    "df[\"home_win\"] = (df[\"margin\"] > 0).astype(int)"
   ]
  },
