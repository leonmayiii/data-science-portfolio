   "source": [
    "# === 6. Plot 4 – Cumulative profit on home underdogs ========================\n",
    "home_dogs = df[df[\"spread_home\"] < 0]\n",
    "stake = 100\n",
    "profit_per_game = np.where(\n",
    "    home_dogs[\"cover\"] == 1,\n",
    "    stake * (home_dogs[\"moneyline_home\"].abs() / 100),  # win\n",
    "    -stake                                              # loss\n",
    ")\n",
    "cum_profit = profit_per_game.cumsum()\n",
    "\n",
    "plt.figure(figsize=(7,4))\n",
    "plt.plot(home_dogs[\"date\"], cum_profit)\n",
    "plt.title(\"Fig 4. Cum. $100 stakes on every home underdog\")\n",
    "plt.xlabel(\"Season\")\n",
    "plt.ylabel(\"Cumulative profit ($)\")\n",
    "plt.axhline(0, color=\"k\", lw=0.8)\n",
    "plt.grid(True)\n",
    "plt.show()"
