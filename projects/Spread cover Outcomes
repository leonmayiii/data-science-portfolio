    "# === 5. Plot 3 – Calibration curve ==========================================\n",
    "bins = pd.qcut(df[\"imp_home_win\"], q=10, duplicates=\"drop\")\n",
    "calib = df.groupby(bins)[\"home_win\"].mean()\n",
    "\n",
    "# FIX: Use calibration index for bin midpoints\n",
    "bin_mid = calib.index.map(lambda interval: interval.mid)\n",
    "\n",
